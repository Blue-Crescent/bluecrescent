<launch>

  <!-- Load the URDF into the ROS Parameter Server -->
  <param name="robot_description"
	 command="$(find xacro)/xacro '$(find bluecrescent_description)/urdf/bluecrescent.xacro'" />

  <!-- ros_control rrbot launch file -->
  <include file="$(find bluecrescent_control)/launch/bluecrescent_control.launch">
  </include>

</launch>
